{
  "name": "peek-history",
  "version": "2.0.1",
  "description": "A simple and minimal extension for quickly peeking and managing history.",
  "author": "AbdulSamad Ansari",
  "license": "GPL-3.0",
  "scripts": {
    "start": "nodemon",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.js",
    "predeploy-build": "yarn run build",
    "deploy-build": "yarn run build && bestzip ${npm_package_name}-v${npm_package_version}.zip dist/*"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "dayjs": "^1.11.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hotkeys-hook": "^4.3.7",
    "react-redux": "^8.0.5",
    "timeago-react": "^3.0.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.217",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "bestzip": "^2.2.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fs-extra": "^11.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.5"
  },
  "engines": {
    "node": "^18.x.x",
    "yarn": "^3.x.x"
  },
  "packageManager": "yarn@3.4.1"
}
